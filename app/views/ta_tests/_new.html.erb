<%= form_for([@professor, @course, @ta_test]) do |f| %>
  <div class="field">
    <%= f.text_field :name, :placeholder => "Name" %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
<div class="actions">
  <%= button_to 'Cancel', professor_course_ta_tests_path, :method => :get %>
</div>

