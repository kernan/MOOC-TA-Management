<h1>Listing ta_tests</h1>
 
<table>
  <tr>
    <th>Name</th>
  </tr>
 
<% @ta_tests.each do |ta_test| %>
  <tr>
    <td><%= ta_test.name %></td>
    <td><%= link_to 'Show', ta_test %></td>
    <td><%= link_to 'Destroy', ta_test_path(ta_test), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
 
<br />
 
<%= link_to 'New TA Test', new_ta_test_path %>
